{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "home-hub-api-nodeport"
  },
  "spec": {
    "selector": {
      "app": "home-hub-api"
    },
    "ports": [
      {
        "name": "api-hub-http",
        "protocol": "TCP",
        "port": 80,
        "targetPort": 8220
      }
    ],
    "type": "NodePort"
  }
}
